exec = ( program )->
	screen = for [0...6]
		for [0...50]
			' '
	reg_rect = /rect (\d+)x(\d+)/
	reg_row = /rotate row y=(\d+) by (\d+)/
	reg_col = /rotate column x=(\d+) by (\d+)/
	for cmd in program.split '\n'
		switch
			when res = reg_rect.exec cmd
				for y in [0... +res[2]]
					for x in [0... +res[1]]
						screen[y][x] = '1'
			when res = reg_row.exec cmd
				y = +res[1]
				d = +res[2]
				screen[y] = screen[y][-d..].concat screen[y][...-d]
			when res = reg_col.exec cmd
				x = +res[1]
				d = +res[2]
				col = for y in [0...6]
					screen[(y-d)%%6][x]
				for v, y in col
					screen[y][x] = v
			else
				throw new Error 'invalid input'

	sum = 0
	for row in screen
		_log row.join ''
		for v in row
			sum+= +v

	sum


