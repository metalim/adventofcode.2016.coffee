limit = ( a, b, x )->
	Math.max a, Math.min b, x

dirs =
	L:[0,-1]
	R:[0,1]
	U:[1,-1]
	D:[1,1]

get_code = ( str )->
	ins = str.split '\n'
	pos = [1,1]
	code = ''
	for row in ins
		for d in row
			dd = dirs[d]
			pos[dd[0]] = limit 0, 2, pos[dd[0]] + dd[1]
		code += (1 + pos[0] + 3*pos[1])
	code

# with extra space at the beginning, for coffee to parse it properly.
pad ='''
