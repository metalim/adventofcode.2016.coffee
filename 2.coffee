'''.split '\n'

get_code2 = ( str )->
	ins = str.split '\n'
	pos = [0,2]
	code = ''
	for row in ins
		for d in row
			dd = dirs[d]
			np = pos[..]
			np[dd[0]] += dd[1]
			if pad[np[1]]?[np[0]] in '123456789ABCD'
				pos = np
		code += pad[pos[1]][pos[0]]
	code

_log get_code str
for row in pad
